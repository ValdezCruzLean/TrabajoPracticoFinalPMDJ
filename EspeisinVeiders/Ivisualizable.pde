/**
  La interfaz IVisualizable define el método display() para objetos que pueden ser visualizados.
  Las clases que implementen esta interfaz deben proporcionar una implementación del método display().
 */
interface IVisualizable {
  
  /**
    El método display() muestra el objeto en la pantalla.
    Las clases que implementen esta interfaz deben proporcionar una implementación de este método.
   */
 public void display();
}
